<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageId>EdsDcfNet</PackageId>
    <Version>1.0.0</Version>
    <Authors>Your Name</Authors>
    <Description>A comprehensive C# .NET library for reading and writing CiA DS 306 EDS (Electronic Data Sheet) and DCF (Device Configuration File) files for CANopen devices.</Description>
    <PackageTags>CANopen;EDS;DCF;CiA;DS306;Industrial Automation</PackageTags>
    <RepositoryUrl>https://github.com/dborgards/eds-dcf-net</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

</Project>
